{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/sub-app/assets/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|webp)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/assets/$1.$2"
    },
    {
      "src": "/assets/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|webp)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/assets/$1.$2"
    },
    {
      "src": "/sub-app/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}