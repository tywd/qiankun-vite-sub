{
  "version": 1,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/sub-app/assets/(.*\\.(js|css|mjs|wasm|json|ico|png|jpg|jpeg|gif|svg))$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript"
      },
      "dest": "/sub-app/assets/$1"
    },
    {
      "src": "^/sub-app/(.*)$",
      "dest": "/sub-app/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
