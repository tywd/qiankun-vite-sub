{
  "name": "qiankun-vite-sub",
  "description": "<%= projectDescription %>",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --port 8081 --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --port 8081",
    "clean": "rm -rf dist node_modules/.vite",
    "build:analyze": "vite build --mode analyze",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint . && stylelint src/**/*.{css,scss,sass,less}",
    "lint:fix": "eslint --fix . && stylelint --fix src/**/*.{css,scss,sass,less}",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/tsconfig": "^0.5.1",
    "element-plus": "^2.11.1",
    "pinia": "^3.0.4",
    "terser": "^5.43.1",
    "typescript": "~5.4.5",
    "vite": "^5.4.10",
    "vite-plugin-qiankun": "^1.0.10",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.0.29"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@tywd/shared-configs": "^1.0.4",
    "commitlint": "^17.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "nanoid": "^5.1.6",
    "prettier": "^2.0.0",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "stylelint": "^14.0.0",
    "vitepress": "^1.6.4",
    "vue-eslint-parser": "^10.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml,yaml}": [
      "prettier --write"
    ]
  }
}